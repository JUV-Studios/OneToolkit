<ContentDialog
	x:Class="OneToolkit.Showcase.Dialogs.ProtocolShareDialog"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:local="using:OneToolkit.Showcase.Dialogs"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	x:Uid="ProtocolShareDialog"
	Style="{StaticResource DefaultContentDialogStyle}"
	mc:Ignorable="d">

	<StackPanel>
		<Button x:Uid="ShareWebLinkButton" />
		<Button x:Uid="ShareLocalLinkButton" />
		<Button Content="{Binding Source='CancelText', Converter={StaticResource ResourceLocalizationConverter}}" />
	</StackPanel>
</ContentDialog>
